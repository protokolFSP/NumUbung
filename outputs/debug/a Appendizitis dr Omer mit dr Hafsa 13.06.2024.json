  "segments": [
    {
      "start": 0.0,
      "end": 1.0,
      "text": " Entschuldigung."
    },
    {
      "start": 1.0,
      "end": 2.0,
      "text": " Bitte schön."
    },
    {
      "start": 2.0,
      "end": 3.0,
      "text": " Guten Abend."
    },
    {
      "start": 3.0,
      "end": 5.0,
      "text": " Mein Name ist Frau Schäker."
    },
    {
      "start": 5.0,
      "end": 7.0,
      "text": " Ich bin hier als Assistentin tätig."
    },
    {
      "start": 7.0,
      "end": 10.0,
      "text": " Heute möchte ich mit Ihnen das Aufnahmegespräch durchführen."
    },
    {
      "start": 10.0,
      "end": 12.0,
      "text": " Sind Sie einverstanden?"
    },
    {
      "start": 12.0,
      "end": 14.0,
      "text": " Ja, hallo. Guten Abend."
    },
    {
      "start": 14.0,
      "end": 16.0,
      "text": " Wie ist Ihr Name nochmal, bitte?"
    },
    {
      "start": 16.0,
      "end": 18.0,
      "text": " Mein Name ist Schäker."
    },
    {
      "start": 18.0,
      "end": 21.0,
      "text": " Schäker. Was ist das für ein Name?"
    },
    {
      "start": 21.0,
      "end": 24.0,
      "text": " Schäker ist eine türkische Nachname."
    },
    {
      "start": 24.0,
      "end": 26.0,
      "text": " Ich komme aus der Türkei."
    },
    {
      "start": 26.0,
      "end": 27.0,
      "text": " Ach so."
    },
    {
      "start": 27.0,
      "end": 29.0,
      "text": " Okay, herzlich willkommen."
    },
    {
      "start": 29.0,
      "end": 31.0,
      "text": " Danke schön."
    },
    {
      "start": 31.0,
      "end": 33.0,
      "text": " Bevor wir anfangen, muss ich schnell"
    },
    {
      "start": 33.0,
      "end": 35.0,
      "text": " an persönliche Fragen stellen."
    },
    {
      "start": 35.0,
      "end": 37.0,
      "text": " Wie heißen Sie?"
    },
    {
      "start": 37.0,
      "end": 42.5,
      "text": " Mein Name ist Melanie Kant."
    },
    {
      "start": 42.5,
      "end": 47.3,
      "text": " Frau Kant, könnten Sie Ihre Formen"
    },
    {
      "start": 47.3,
      "end": 49.3,
      "text": " machen, mal langsam, büchtig?"
    },
    {
      "start": 49.3,
      "end": 55.3,
      "text": " Melanie, Martha, Emil, Ludwig, Anton,"
    },
    {
      "start": 55.3,
      "end": 62.64,
      "text": " Nordpol, Ida, Emil, Kant, Karl, Anton,"
    },
    {
      "start": 62.64,
      "end": 64.64,
      "text": " Volteo, Ida."
    },
    {
      "start": 64.64,
      "end": 66.64,
      "text": " Okay."
    },
    {
      "start": 66.64,
      "end": 68.64,
      "text": " Frau Kant, wie alt sind Sie?"
    },
    {
      "start": 68.64,
      "end": 71.64,
      "text": " Ich bin 46 Jahre alt."
    },
    {
      "start": 71.64,
      "end": 73.64,
      "text": " 46."
    },
    {
      "start": 73.64,
      "end": 75.64,
      "text": " Okay. Und wann genau sind Sie geboren?"
    },
    {
      "start": 75.64,
      "end": 82.64,
      "text": " Am 23. März, 1978."
    },
    {
      "start": 82.64,
      "end": 84.64,
      "text": " 78."
    },
    {
      "start": 84.64,
      "end": 86.64,
      "text": " Okay."
    },
    {
      "start": 86.64,
      "end": 89.64,
      "text": " Und wie groß sind Sie?"
    },
    {
      "start": 89.64,
      "end": 92.64,
      "text": " Frau Doktor, ich komme hier, weil ich beschwerden habe,"
    },
    {
      "start": 92.64,
      "end": 94.64,
      "text": " Sie fragen nach meiner Größe."
    },
    {
      "start": 94.64,
      "end": 96.64,
      "text": " Ich verstehe das nicht."
    },
    {
      "start": 96.64,
      "end": 99.64,
      "text": " Ja, ich verstehe, aber ich muss diese Fragen stellen,"
    },
    {
      "start": 99.64,
      "end": 104.64,
      "text": " um zum Beispiel Medikamente-Dosis zu berechnen."
    },
    {
      "start": 104.64,
      "end": 106.64,
      "text": " Okay."
    },
    {
      "start": 106.64,
      "end": 110.64,
      "text": " Ich bin, ich glaube, 1,65 m."
    },
    {
      "start": 110.64,
      "end": 114.64,
      "text": " Sie wollen sicherlich auch meinen Gewicht wissen."
    },
    {
      "start": 114.64,
      "end": 116.64,
      "text": " Könnten Sie wiederholen?"
    },
    {
      "start": 116.64,
      "end": 119.64,
      "text": " Sie wollen wahrscheinlich auch meinen Gewicht wissen."
    },
    {
      "start": 119.64,
      "end": 121.64,
      "text": " Ja."
    },
    {
      "start": 121.64,
      "end": 123.64,
      "text": " Ich bin 72 kg."
    },
    {
      "start": 123.64,
      "end": 125.64,
      "text": " 71."
    },
    {
      "start": 125.64,
      "end": 127.64,
      "text": " Okay."
    },
    {
      "start": 127.64,
      "end": 129.64,
      "text": " Gut."
    },
    {
      "start": 129.64,
      "end": 131.64,
      "text": " Frau Kant, was führt Sie uns?"
    },
    {
      "start": 131.64,
      "end": 133.64,
      "text": " Wie kann ich Ihnen helfen?"
    },
    {
      "start": 133.64,
      "end": 135.64,
      "text": " Frau Doktor, ich habe seit heute Mittag"
    },
    {
      "start": 135.64,
      "end": 138.64,
      "text": " starke Schmerzen auf der rechten Seite vom Bau."
    },
    {
      "start": 138.64,
      "end": 146.12,
      "text": " Ober- oder unterer Bereich?"
    },
    {
      "start": 146.12,
      "end": 148.12,
      "text": " Das ist schwer zu starten."
    },
    {
      "start": 148.12,
      "end": 153.12,
      "text": " Es ist mehr im Unterbereich, im Unterbauch."
    },
    {
      "start": 153.12,
      "end": 158.12,
      "text": " Und es strahlt so ein bisschen in meine Leiste,"
    },
    {
      "start": 158.12,
      "end": 160.12,
      "text": " als im Unterbauch."
    },
    {
      "start": 160.12,
      "end": 163.12,
      "text": " Es strahlt von der rechten Seite in den Mittelbauch,"
    },
    {
      "start": 163.12,
      "end": 165.12,
      "text": " in die Mitte."
    },
    {
      "start": 165.12,
      "end": 169.12,
      "text": " Es sind zum Teil Krämpfe, manchmal sind es Schmerzen."
    },
    {
      "start": 169.12,
      "end": 171.12,
      "text": " Ich weiß nicht, wie ich es beschreiben soll."
    },
    {
      "start": 171.12,
      "end": 173.12,
      "text": " Einfach ganz komisch, aber sehr, sehr schmerzhaft."
    },
    {
      "start": 173.12,
      "end": 175.12,
      "text": " Es nimmt immer zu."
    },
    {
      "start": 175.12,
      "end": 177.12,
      "text": " Okay, ich verstehe."
    },
    {
      "start": 177.12,
      "end": 183.33,
      "text": " Haben diese Schmerzen plötzlich begonnen oder eher langsam?"
    },
    {
      "start": 183.33,
      "end": 187.33,
      "text": " Ich würde sagen eher plötzlich."
    },
    {
      "start": 187.33,
      "end": 190.33,
      "text": " Das fing heute Mittag an, vorher hatte ich gar nichts."
    },
    {
      "start": 190.33,
      "end": 193.33,
      "text": " Jetzt ist es richtig schlimm geworden,"
    },
    {
      "start": 193.33,
      "end": 195.33,
      "text": " sodass ich herkommen musste."
    },
    {
      "start": 195.33,
      "end": 202.5,
      "text": " Gibt es bestimmte Auslöse, haben Sie bemerkt zum Beispiel Essen?"
    },
    {
      "start": 202.5,
      "end": 204.5,
      "text": " Nein, kann ich nicht sagen."
    },
    {
      "start": 204.5,
      "end": 206.5,
      "text": " Das fing heute Mittag an."
    },
    {
      "start": 206.5,
      "end": 210.5,
      "text": " Ich habe auch nichts Besonderes gemacht."
    },
    {
      "start": 210.5,
      "end": 215.5,
      "text": " Ich war zu Hause, habe nur ein Buch gelesen"
    },
    {
      "start": 215.5,
      "end": 218.5,
      "text": " und dann fing das an."
    },
    {
      "start": 218.5,
      "end": 220.5,
      "text": " Okay, ich verstehe."
    },
    {
      "start": 220.5,
      "end": 223.5,
      "text": " Kennen Sie diese Schmerzen? Hatten Sie früher schon einmal?"
    },
    {
      "start": 223.5,
      "end": 225.5,
      "text": " Nein, überhaupt nicht."
    },
    {
      "start": 225.5,
      "end": 228.5,
      "text": " Das erste Mal, dass ich solche starke Schmerzen habe."
    },
    {
      "start": 228.5,
      "end": 230.5,
      "text": " Okay."
    },
    {
      "start": 230.5,
      "end": 233.5,
      "text": " Könnten Sie die Schmerzen genauer beschreiben?"
    },
    {
      "start": 233.5,
      "end": 236.5,
      "text": " Sie haben schon krampfig gesagt,"
    },
    {
      "start": 236.5,
      "end": 239.5,
      "text": " aber zum Beispiel Birnen, Stehen oder?"
    },
    {
      "start": 239.5,
      "end": 243.5,
      "text": " Also brennend jetzt nicht, aber zum Teil sticht das."
    },
    {
      "start": 243.5,
      "end": 245.5,
      "text": " Das ist richtig."
    },
    {
      "start": 245.5,
      "end": 248.5,
      "text": " Der strahlt so ein bisschen in den Unterbauch aus, in die Mitte."
    },
    {
      "start": 248.5,
      "end": 253.5,
      "text": " Ich habe auch eine leichte Übelkeit, seitdem merke ich jetzt."
    },
    {
      "start": 253.5,
      "end": 256.5,
      "text": " Kann sie sich übergeben?"
    },
    {
      "start": 256.5,
      "end": 260.5,
      "text": " Nein, das noch nicht, aber die Übelkeiten nehmen zu."
    },
    {
      "start": 260.5,
      "end": 266.5,
      "text": " Und zum Teil sind die Schmerzen auch so ein bisschen krampfartig."
    },
    {
      "start": 266.5,
      "end": 269.5,
      "text": " Das ist mal mehr, mal weniger."
    },
    {
      "start": 269.5,
      "end": 272.5,
      "text": " Also sehr unangenehm."
    },
    {
      "start": 272.5,
      "end": 276.5,
      "text": " Ja, okay. Wie würden Sie die Schmerzen auf einer Schmerzzucker"
    },
    {
      "start": 276.5,
      "end": 279.5,
      "text": " von 1 bis 10 bewerten, wenn 1 leichte Schmerzen und 10"
    },
    {
      "start": 279.5,
      "end": 282.5,
      "text": " starkeste Schmerzen benötet?"
    },
    {
      "start": 282.5,
      "end": 285.5,
      "text": " Ach Gott, ich würde sagen, es hat mit 2, 3 angefangen,"
    },
    {
      "start": 285.5,
      "end": 290.5,
      "text": " ist jetzt bei wahrscheinlich 5 oder 6 oder vielleicht sogar mehr."
    },
    {
      "start": 290.5,
      "end": 300.24,
      "text": " Okay."
    }
  ]
}